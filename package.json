{
  "name": "novel-editor",
  "version": "0.0.0",
  "main": "dist-electron/main/index.js",
  "description": "小说编辑器",
  "author": "ljw1412",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build:vite": "vue-tsc --noEmit && vite build",
    "build": "electron-builder build --config electron-builder.config.js --dir",
    "compile": "npm run build:vite && npm run build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/d3": "^7.4.0",
    "@types/qs": "^6.9.7",
    "@vitejs/plugin-vue": "^4.1.0",
    "autoprefixer": "^10.4.14",
    "electron": "^22.3.5",
    "electron-builder": "^23.6.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-json-storage": "^4.6.0",
    "postcss": "^8.4.21",
    "qs": "^6.11.1",
    "sass": "^1.60.0",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.2",
    "vite": "4.1.4",
    "vite-plugin-electron": "^0.11.1",
    "vite-plugin-electron-renderer": "^0.13.14",
    "vite-plugin-html-banner": "^0.0.3",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue-tsc": "^1.2.0"
  },
  "dependencies": {
    "@arco-design/web-vue": "^2.44.7",
    "@vueuse/core": "^9.13.0",
    "d3": "^7.8.4",
    "pinia": "^2.0.33",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  }
}
